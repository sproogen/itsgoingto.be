{
  "name": "itsgoingtobe-react",
  "version": "0.0.1",
  "engines": {
    "node": "14.16.x"
  },
  "author": "James Grant <james@jwgmedia.co.uk> (http://jameswgrant.co.uk)",
  "license": "GPLv3",
  "description": "It's Going To Be front-end with React, Redux, and React-Router!",
  "scripts": {
    "postinstall": "",
    "start": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --config webpack.config.ts",
    "build": "webpack --config webpack.config.ts",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:stats": "cross-env NODE_ENV=production npm run build -- --profile --json > build/stats.json",
    "test": "jest",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint-output": "eslint-output",
    "analyze": "webpack-bundle-analyzer build/stats.json",
    "storybook": "start-storybook -p 6006 --quiet",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "antd": "^3.6.4",
    "autosize": "^4.0.2",
    "cancelable-promise": "^2.4.0",
    "classnames": "^2.3.1",
    "core-js": "^3.6.4",
    "fbemitter": "^2.1.1",
    "font-awesome": "^4.7.0",
    "moment": "^2.22.2",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "ramda": "^0.25.0",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-countdown-now": "^1.2.0",
    "react-datepicker": "^0.58.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "react-linkify": "^0.2.1",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.17",
    "@k88/interpolate-html-plugin": "^1.0.5",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/fbemitter": "^2.0.32",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^26.0.23",
    "@types/mini-css-extract-plugin": "^1.4.2",
    "@types/node": "^15.0.1",
    "@types/postcss-flexbugs-fixes": "^4.2.0",
    "@types/postcss-preset-env": "^6.7.2",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.4",
    "@types/react-dev-utils": "^9.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.0",
    "@types/webpack-dev-server": "^3.11.3",
    "@types/webpack-manifest-plugin": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-typescript-to-proptypes": "^1.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-output": "^2.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "fork-ts-checker-webpack-plugin": "^6.2.5",
    "html-webpack-plugin": "^4.5.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "postcss": "^7.0.27",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react-dev-utils": "^11.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^3.1.0",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*?(*.)(spec|test).js"
    ],
    "transform": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test-utils/file-transform.js",
      "\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|scss)$": "<rootDir>/test-utils/style-mock.js"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test-utils/test-setup.js"
    ],
    "coverageDirectory": "build/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/scripts/",
      "<rootDir>/.storybook/",
      "<rootDir>/stories/",
      "<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "lcov"
    ]
  }
}
